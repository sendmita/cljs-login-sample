(ns hellominion.core
  (:require [reagent.core :as reagent]))

(def app-state
  (reagent/atom
    {:message "HELLO MINIONS"
      :correctuser "mini"
      :correctpwd "mouse"
      :minionclass "hideMinion"
    })

)

(defn showminion []
  (reset! app-state {:message "HELLO FROM MINION"
    :correctuser "mini"
    :correctpwd "mouse"
    :minionclass "showMinion"
  })
)

(defn app []
  [:div {}
    [:h1 {:class "title"} (:message @app-state)]
    [:div {:id "login"}
      "username:" [:input {:id "username" :type "text"}]
      [:br][:br]
      "password:" [:input {:id "password" :type "text"}]
      [:br][:br]
      [:button {:type "button" :onClick (fn [event]
              (when (and (== (.-value (js/document.getElementById "username")) (:correctuser @app-state))
                        (== (.-value (js/document.getElementById "password")) (:correctpwd @app-state)))
                (showminion)
              )
          )
        }
        "Click Me!"]
      [:br][:br]
      [:div {:id "minionImg" :class (:minionclass @app-state)}
        [:img {:src "data:image/jpeg;base64,"}]
      ]
    ]
  ]
)


(reagent/render [app] (js/document.querySelector "#cljs-target"))
